package com.sample.dto;

import java.util.List;
import java.util.Map;

import com.sample.web.view.Mate;
import com.sample.web.view.MateTimeLine;

public class MateDetailDto {

	private Mate mate;
	private List<MateTimeLine> timeline;
	private Map<Object, String> mateCategory;
	
	
	public MateDetailDto() {
	}
	
	
	
	public Map<Object, String> getMateCategory() {
		return mateCategory;
	}


	public void setMateCategory(Map<Object, String> mateCategory) {
		this.mateCategory = mateCategory;
	}


	public Mate getMate() {
		return mate;
	}

	public void setMate(Mate mate) {
		this.mate = mate;
	}

	public List<MateTimeLine> getTimeline() {
		return timeline;
	}

	public void setTimeline(List<MateTimeLine> timeline) {
		this.timeline = timeline;
	}

	@Override
	public String toString() {
		return "MateDetailDto [mate=" + mate + ", timeline=" + timeline + "]";
	}
	
	
	
}
